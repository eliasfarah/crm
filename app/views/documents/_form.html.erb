<% form_for([@project,@document], :html=>{ :multipart=>true }) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :project_id %><br />
    <%= f.collection_select :project_id, @projects, :id, :name %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
	<p>
		<%= f.label :document %><br />
		<%= f.file_field :document %>
	</p>
  <p>
    <%= f.submit 'Save' %>
  </p>
<% end %>